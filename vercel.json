{
  "version": 2,
  "builds": [
    {
      "src": "api/index.php",
      "use": "vercel-php@0.7.1"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/build/(.*)",
      "dest": "public/build/$1"
    },
    {
      "src": "/images/(.*)",
      "dest": "public/images/$1"
    },
    {
      "src": "/(robots\\.txt|favicon\\.ico)",
      "dest": "public/$1"
    },
    {
      "src": "/(.*)",
      "dest": "api/index.php"
    }
  ],
  "env": {
    "APP_ENV": "production",
    "APP_DEBUG": "true",
    "APP_KEY": "base64:HoithTic+FgtAFxX1j4X+VckLUhcuikV7dct1bOq7qg=",
    "APP_URL": "https://devinasystem.vercel.app",
    "LOG_CHANNEL": "stderr",
    "SESSION_DRIVER": "array",
    "CACHE_DRIVER": "array",
    "QUEUE_CONNECTION": "sync",
    "DB_CONNECTION": "sqlite",
    "DB_DATABASE": "/tmp/database.sqlite",
    "VIEW_COMPILED_PATH": "/tmp/storage/framework/views",
    "APP_STORAGE": "/tmp/storage"
  },
  "build": {
    "env": {
      "VERCEL": "1"
    }
  }
}