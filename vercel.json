{
  "version": 2,
  "builds": [
    {
      "src": "api/index.php",
      "use": "vercel-php@0.7.1"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/build/(.*)",
      "dest": "public/build/$1"
    },
    {
      "src": "/images/(.*)",
      "dest": "public/images/$1"
    },
    {
      "src": "/(robots\\.txt|favicon\\.ico)",
      "dest": "public/$1"
    },
    {
      "src": "/(.*)",
      "dest": "api/index.php"
    }
  ],
  "env": {
    "APP_NAME": "Car Rental System",
    "APP_ENV": "production",
    "APP_DEBUG": "true",
    "APP_KEY": "base64:kmgc+nEwNHN0xcORP37dhXrtELuP5yEZO96+lKY8CRY=",
    "APP_TIMEZONE": "UTC",
    "APP_URL": "https://devinasystem.vercel.app",
    "LOG_CHANNEL": "stderr",
    "LOG_LEVEL": "debug",
    "SESSION_DRIVER": "array",
    "CACHE_DRIVER": "array",
    "QUEUE_CONNECTION": "sync",
    "BROADCAST_CONNECTION": "log",
    "FILESYSTEM_DISK": "local",
    "DB_CONNECTION": "sqlite",
    "DB_DATABASE": "/tmp/database.sqlite",
    "VIEW_COMPILED_PATH": "/tmp/storage/framework/views",
    "APP_SERVICES_CACHE": "",
    "APP_PACKAGES_CACHE": "",
    "APP_CONFIG_CACHE": "",
    "APP_ROUTES_CACHE": "",
    "APP_EVENTS_CACHE": ""
  },
  "build": {
    "env": {
      "VERCEL": "1"
    }
  }
}